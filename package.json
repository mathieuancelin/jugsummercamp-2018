    "jquery": "3.3.1",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "minimist": "1.2.0",
    "moment": "2.22.2",
    "node-fetch": "2.2.0",
    "react": "16.4.2",
    "react-datetime": "2.15.0",
    "react-dom": "16.4.2",
    "react-izanami": "^1.0.6",
